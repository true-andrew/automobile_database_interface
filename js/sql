SELECT Номера.[Тип номера], Номера.[Стоимость за ночь], Номера.Номер
FROM Номера INNER JOIN Бронирование ON Номера.Номер = Бронирование.[Номер помещения]
WHERE ([Дата прибытия] NOT BETWEEN Бронирование.[Дата заезда] AND Бронирование.[Дата выезда]) AND ([Дата отбытия] NOT BETWEEN Бронирование.[Дата заезда] AND Бронирование.[Дата выезда]);




SELECT Номера.[Тип номера], Номера.[Стоимость за ночь], Номера.Номер
FROM Номера INNER JOIN Бронирование ON Номера.Номер = Бронирование.[Номер помещения]
WHERE ((([Дата прибывания]) Not Between [Бронирование].[Дата заезда] And [Бронирование].[Дата выезда]));
